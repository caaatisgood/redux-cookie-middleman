    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-typescript": "7.1.0",
    "@types/jest": "23.3.12",
    "@types/lodash.omit": "4.5.4",
    "@types/lodash.omitby": "4.6.4",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3",
    "rollup": "1.0.2",
