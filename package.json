    "has": "1.0.3",
    "js-cookie": "2.2.0",
    "lodash.omit": "4.5.0",
    "lodash.omitby": "4.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "git add"
    ]
