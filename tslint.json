{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "jsRules": {},
  "linterOptions": {
    "exclude": [
      "package.json"
    ]
  },
  "rules": {
    "prettier": true,
    "import-name": false,
    "variable-name": false,
    "trailing-comma": true
  },
  "rulesDirectory": []
}